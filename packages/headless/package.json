{
  "name": "@framv/headless",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "framv": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "description": "Headless renderer for framv â€” render HTML/SVG elements to images or video via Puppeteer",
  "keywords": [
    "puppeteer",
    "headless",
    "renderer",
    "video",
    "png",
    "svg",
    "mp4",
    "framv",
    "cli"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@framv/core": "*",
    "puppeteer": "^24.37.5"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@vitest/ui": "^4.0.18",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
